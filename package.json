{
  "name": "my-turborepo",
  "private": true,
  "scripts": {
    "build": "turbo build",
    "dev": "turbo dev",
    "lint": "turbo lint",
    "lint-fix": "turbo lint-fix",
    "typecheck": "turbo typecheck",
    "test": "turbo test",
    "lint-staged": "turbo lint-staged --concurrency=1",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.2",
    "prettier": "3.3.3",
    "turbo": "2.1.2",
    "typescript": "5.6.2"
  },
  "packageManager": "pnpm@9.11.0",
  "engines": {
    "node": ">=22"
  }
}
