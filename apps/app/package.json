{
  "name": "@repo/app",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "packageManager": "pnpm@9.9.0",
  "scripts": {
    "dev": "dotenv -e .env.local -- remix vite:dev",
    "build": "dotenv -e .env.production -- remix vite:build",
    "deploy-cf": "dotenv -e .env.production -- npm run build && wrangler pages deploy",
    "preview": "npm run build && wrangler pages dev",
    "lint": "eslint . && biome check",
    "lint-fix": "eslint . --fix && biome check --write --unsafe",
    "typecheck": "tsc",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "test": "dotenv -e .env.test -- vitest --workspace=vitest.workspace.ts",
    "pretest": "pnpm exec playwright install chromium",
    "gen-c": "scaffdog generate react-component"
  },
  "dependencies": {
    "@hookform/resolvers": "3.9.0",
    "@prisma/client": "5.19.1",
    "@prisma/instrumentation": "5.19.1",
    "@radix-ui/react-icons": "1.3.0",
    "@remix-run/cloudflare": "2.12.1",
    "@remix-run/cloudflare-pages": "2.12.1",
    "@remix-run/react": "2.12.1",
    "@tanstack/react-query": "5.55.0",
    "@trpc/client": "11.0.0-rc.502",
    "@trpc/react-query": "11.0.0-rc.502",
    "class-variance-authority": "0.7.0",
    "clsx": "2.1.1",
    "date-fns": "4.1.0",
    "dotenv-cli": "7.4.2",
    "isbot": "5.1.17",
    "lucide-react": "0.438.0",
    "prisma": "5.19.1",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-hook-form": "7.53.0",
    "tailwind-merge": "2.5.2",
    "tailwindcss-animate": "1.0.7",
    "valibot": "0.41.0"
  },
  "devDependencies": {
    "@biomejs/biome": "1.8.3",
    "@cloudflare/workers-types": "4.20240925.0",
    "@remix-run/dev": "2.11.2",
    "@remix-run/testing": "2.12.0",
    "@storybook/addon-essentials": "8.2.9",
    "@storybook/addon-interactions": "8.2.9",
    "@storybook/addon-links": "8.2.9",
    "@storybook/blocks": "8.2.9",
    "@storybook/react": "8.2.9",
    "@storybook/react-vite": "8.2.9",
    "@storybook/test": "8.2.9",
    "@testing-library/react": "16.0.1",
    "@trpc/server": "11.0.0-rc.502",
    "@types/react": "18.3.5",
    "@types/react-dom": "18.3.0",
    "@typescript-eslint/parser": "8.4.0",
    "@vitejs/plugin-react": "4.3.1",
    "@vitest/browser": "2.0.5",
    "autoprefixer": "10.4.20",
    "eslint": "9.9.1",
    "eslint-plugin-tailwindcss": "3.17.4",
    "globals": "15.9.0",
    "lefthook": "1.7.15",
    "npm-run-all": "4.1.5",
    "playwright": "1.47.0",
    "postcss": "8.4.45",
    "scaffdog": "4.0.0",
    "storybook": "8.2.9",
