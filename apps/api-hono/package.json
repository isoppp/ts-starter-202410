{
  "name": "@repo/api-hono",
  "scripts": {
    "dev": "dotenv -e .env.local -- tsx watch src/index.ts",
    "dev-email": "dotenv -e .env.local -- email dev --dir ./srcfeature/email/components",
    "export-type": "dotenv -e .env.local -- tsc --project tsconfig.export.json",
    "export-type2": "tsc src/trpc/index.type.ts --declaration --emitDeclarationOnly --outDir ./types",
    "migrate": "dotenv -e .env.local -- prisma migrate dev",
    "migrate:reset": "dotenv -e .env.local -- prisma migrate reset",
    "prisma:studio": "dotenv -e .env.development -- prisma studio",
    "seed": "dotenv -e .env.local -- tsx prisma/seed/index.ts",
    "postinstall": "prisma generate",
    "typecheck": "tsc"
  },
  "dependencies": {
    "@google-cloud/error-reporting": "3.0.5",
    "@google-cloud/logging": "11.2.0",
    "@google-cloud/logging-winston": "6.0.0",
    "@google-cloud/opentelemetry-cloud-trace-exporter": "2.3.0",
    "@hono/node-server": "1.13.1",
    "@hono/trpc-server": "0.3.2",
    "@opentelemetry/api": "1.9.0",
    "@opentelemetry/instrumentation-http": "0.53.0",
    "@opentelemetry/instrumentation-winston": "0.40.0",
    "@opentelemetry/sdk-node": "0.53.0",
    "@prisma/client": "5.19.1",
    "@prisma/instrumentation": "5.19.1",
    "@react-email/components": "0.0.25",
    "@trpc/server": "11.0.0-rc.502",
    "chalk": "5.3.0",
    "date-fns": "3.6.0",
    "dotenv-cli": "7.4.2",
    "hono": "4.6.3",
    "hono-rate-limiter": "0.4.0",
    "prisma": "5.19.1",
    "react": "18.3.1",
    "react-email": "3.0.1",
    "tsx": "4.19.1",
    "valibot": "0.41.0",
    "winston": "3.14.2"
  },
  "devDependencies": {
    "@types/node": "22.6.1",
    "@types/react": "18.3.5",
    "@vitest/browser": "2.0.5",
    "tsup": "8.3.0",
    "typescript": "5.6.2",
    "vitest": "2.0.5"
  }
}
